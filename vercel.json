{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/status",
      "destination": "https://status.mynco.app",
      "permanent": false
    },
    {
      "source": "/changelog",
      "destination": "https://changelog.mynco.app",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
} 